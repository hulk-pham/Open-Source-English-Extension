{
  "manifest_version": 3,
  "name": "Word Saver",
  "description": "A chrome extension to learn english",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}